{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/server/index.ts",
    "start": "NODE_ENV=production bun src/server/index.ts",
    "build": "bun run build.ts",
    "test": "bun test --preload ./tests/setup.ts",
    "migration:generate": "bunx drizzle-kit generate --name=$1",
    "db:migrate": "bunx drizzle-kit migrate",
    "docker:dev": "docker compose -f docker-compose.yml up -d --build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-router": "^1.133.36",
    "@tanstack/router-devtools": "^1.133.36",
    "axios": "^1.13.1",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "lucide-react": "^0.545.0",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "react": "^19",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19",
    "react-hook-form": "^7.66.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.5",
    "happy-dom": "^15.11.7",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
